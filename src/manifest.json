{
  "name": "Git Pullbot Extension",
  
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "imageIcon.png", 
    "default_tittle":"PullBot "
  },
  "web_accessible_resources": [{
    "resources": ["options.html"],
    "matches": ["<all_urls>"],
    "extension_ids": []
   }],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "deion": "Opens bot.html"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["index.js"]
    }
  ]
}