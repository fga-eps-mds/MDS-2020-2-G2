{
  "name": "Git Pullbot Extension",
  "deion": "Uma extensão do chrome para gerenciar repositórios do git, e automatizar o processo do fechamento de milestones",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "imageIcon.png", 
    "default_tittle":"PullBot "
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "deion": "Opens bot.html"
    }
  },
  "content_s": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["index.js"]
    }
  ]
}